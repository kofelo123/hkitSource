package com.jeongwon.io;

import java.io.FileInputStream;
import java.io.FileOutputStream;
import java.io.IOException;

public class FileCopy {
	public static void main(String[] args) {
		try{
			FileInputStream fis = new FileInputStream("src/com/jeongwon/io/FileCopy.java");
			FileOutputStream fos = new FileOutputStream("C:/Users/USER/Desktop/FileCopy2.bak");
			
			int data =0;
			while((data=fis.read())!=-1){
				fos.write(data);
			}
			fis.close();
			fos.close();
			
		}catch(IOException e){
			e.printStackTrace();
		}
	}
}
