		int[] ball3 = new int[3];
		
		for(int i=0;i<ballArr.length;i++){
			int j = (int) (Math.random() * ballArr.length);
			int tmp = 0;//?
			int count=0;
			tmp=ballArr[j];
			for(int k=0;k<3;k++){
				if(tmp==ball3[k])
					count++;
				if(count==0)
					
			}
		}
	}

}
