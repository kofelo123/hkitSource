package com.jeongwon.ch12;

enum Gender{
	MAIL,FEMAIL;
}
public class Enum {

	public static final String MAIL = "MAIL";
	public static final String FEMAIL = "FEMAIL";
	
	public static void main(String[] args) {
		String gender1,gender2,gender3;
	
		Gender g1,g2,g3;
		g1 = Gender.FEMAIL;
		g2 = Gender.MAIL;
		
	}
}

