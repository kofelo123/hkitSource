package kr.hkit.Prob2;

public class Prob4_12 {
	public static void main(String[] args) {
		for(int i=2;i<=9;i++){
		for(int count=1;count<=3;count++){
			for(int j=2;j<=4;j++){				
				System.out.print(j+"*"+i+"="+j*i+"\t");
			}
			System.out.println();
		}
		System.out.println();
		System.out.println();
		}
	}
}
