	public static void main(String[] args) {

		int[] answer = {1,4,4,3,1,4,4,2,1,3,2};
		int[] counter = new int[4];
		
		for(int i=0; i < answer.length; i++){
			//
//			if(answer[i]==)
			
			switch (answer[i]) {
			case 0:
				
				break;
			case 1:
				
				break;
			case 2:
				
				break;
			case 3:
				
				break;

			default:
				break;
			}
		}
		
		for(int i=0; i< counter.length;i++){
			//
			System.out.println();
		}
	}

}
