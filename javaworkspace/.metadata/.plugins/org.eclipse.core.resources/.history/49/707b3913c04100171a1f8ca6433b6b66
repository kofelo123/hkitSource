	public Orange(int sugar){
		sugarContent = sugar;
		}
	
	public void showSugerContent(){
		System.out.println("´çµµ : "+ sugarContent);
	}
}

class FruitBox{
	Orange item;
	public void store(Orange item){
		this.item = item;
		
	}
