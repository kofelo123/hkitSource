package kr.hkit.Prob;

public class FirstProb {
	
	private int numb;
	
	public FirstProb() {
		print();
	}
	
	public void print(){//static으로 해볼것
		int up=0;
		for(int i=1; i<=15; i++){
						
			System.out.print(i+up+", ");
			
		}
		
	}
	public static void main(String[] args) {
	
		FirstProb firstProb = new FirstProb();
		
	}
}
