package kr.hkit.Prob;

public class FirstProb {
	
	private int numb;
	
	public FirstProb() {
		print();
	}
	
	public void print(){//static으로 해볼것
		int j=1;
		for(int i=0; i<=14; i++){
			j+=i;
			System.out.print(j+", ");
			
		}
		
	}
	public static void main(String[] args) {
	
		FirstProb firstProb = new FirstProb();
		
	}
}
