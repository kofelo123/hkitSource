package kr.hkit.array;

public class Lotto {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] six = new int[6];
		int nan,sameCount=0,count=0;
		do{
			nan=(int)(Math.random()*46);//난수발생
			
			do{
				six[count]==nan;
				
			}while(sameCount);
			count++;
		}while(count<six.length);
	}

}
