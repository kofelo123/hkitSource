package com.jeongwon.threaded;

public class ThreadEx8 {
	public static void main(String[] args) {
		
	}

}

class ThreadEx8_1 extends Thread{
	public void run(){
		for(int i=0; i< 300; i++){
			System.out.println("-");
			for(int x=0; x < 10000000; x++);
		}
	}
}


class ThreadEx8_2 extends Thread{
	public void run(){
		for(int i=0; i < 300; i++){
			System.out.println("|");
			for(int x=0; x < 1000000; x++);
		}
	}
}